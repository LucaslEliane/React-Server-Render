{
  "name": "react-isomorphic-boilerplate",
  "version": "1.0.0",
  "description": "An universal React isomorphic boilerplate for building server-side render web app",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon ./server/server.dev.js --watch server --watch build",
    "build": "npm run clean && cross-env NODE_ENV=develepment webpack --config ./build/webpack.prod.config.js --progress",
    "clean": "rimraf ./dist/client/* && rimraf ./dist/server/* && rimraf ./views/prod/*",
    "lint": "eslint . --ext .js",
    "start": "node ./dist/server/server.js",
    "pm2": "pm2"
  },
  "dependencies": {
    "ejs": "^2.5.6",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-compose": "^3.2.0",
    "koa-compress": "^2.0.0",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.0",
    "koa-logger": "^2.0.0",
    "koa-router": "^7.0.0",
    "koa-session": "^3.4.0",
    "koa-static": "^3.0.0",
    "koa-views": "^6.0.1",
    "pm2": "^2.2.2",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^5.0.1",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-action": "^1.2.2",
    "redux-actions": "^2.0.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.1.0",
    "superagent": "^3.3.1"
  },
  "devDependencies": {
    "asset-require-hook": "^1.0.2",
    "autoprefixer": "^6.4.1",
    "babel-core": "^6.13.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.26.0",
    "css-modules-require-hook": "^4.0.5",
    "eslint": "^3.12.2",
    "eslint-plugin-react": "^6.8.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.10.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "jasmine": "^2.6.0",
    "jasmine-core": "^2.6.0",
    "jasmine-es6": "^0.4.0",
    "json-loader": "^0.5.4",
    "karma": "^1.6.0",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "koa-webpack-dev-middleware": "^1.3.0",
    "koa-webpack-hot-middleware": "^1.0.3",
